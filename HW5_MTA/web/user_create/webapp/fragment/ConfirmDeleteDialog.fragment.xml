<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		title="{i18n>confirm}"
		type="Message" >
		<content>
			<Text text="{i18n>confirmDeleteText}" />
		</content>
		<beginButton>
			<Button text="{i18n>delete}" press="onConfirmDelete" />
		</beginButton>
		<endButton>
			<Button text="{i18n>cancel}" press="onCancelDelete" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>